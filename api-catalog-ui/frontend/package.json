{
    "name": "frontend",
    "version": "0.3.0",
    "private": true,
    "homepage": ".",
    "dependencies": {
        "@babel/core": "7.22.10",
        "@emotion/core": "10.1.1",
        "@emotion/styled": "10.0.27",
        "agentkeepalive": "4.1.4",
        "buffer": "6.0.3",
        "emotion-theming": "10.0.27",
        "exception-formatter": "1.0.7",
        "history": "4.10.1",
        "htmr": "1.0.1",
        "install": "0.12.2",
        "lodash": "4.17.21",
        "loglevel": "1.6.1",
        "mineral-ui": "0.58.0",
        "mineral-ui-icons": "0.5.0",
        "ramda": "0.29.1",
        "react": "16.14.0",
        "react-app-polyfill": "0.1.3",
        "react-dom": "16.14.0",
        "react-loadable": "5.5.0",
        "react-redux": "5.1.1",
        "react-router-dom": "4.3.1",
        "react-router-tabs": "1.2.0",
        "react-toastify": "4.4.3",
        "redux": "4.0.1",
        "redux-catch": "1.3.1",
        "redux-logger": "3.0.6",
        "redux-observable": "1.0.0",
        "redux-persist": "5.10.0",
        "redux-persist-transform-filter": "0.0.18",
        "redux-thunk": "2.3.0",
        "request-promise-native": "1.0.9",
        "rxjs": "6.3.3",
        "swagger-ui-react": "4.19.1",
        "util": "0.12.4",
        "uuid": "3.3.2"
    },
    "overrides": {
        "mineral-ui": "0.58.0",
        "@emotion/is-prop-valid": "1.1.0",
        "ua-parser-js": "0.7.34",
        "nth-check": "2.0.1",
        "jsdom": "16.6.0",
        "got": "12.1.0",
        "react-error-overlay": "6.0.9",
        "fast-json-patch": "3.1.1",
        "isomorphic-fetch": "1.7.0",
        "semver": "7.5.4",
        "tough-cookie": "4.1.3",
        "@braintree/sanitize-url": "6.0.4"
    },
    "scripts": {
        "lint": "eslint \"src/**/*.{js,jsx}\"",
        "build": "react-scripts build",
        "postbuild": "rimraf build/**/*.map",
        "test": "react-scripts test --silent --watchAll=false --env=jsdom components/* reducers/* epics/* actions/* selectors/* ErrorBoundary/* --reporters=default --reporters=jest-html-reporter --coverage",
        "cy:open": "cypress open",
        "cy:e2e:ci": "cypress run --spec \"cypress/integration/e2e/**/*.test.js\" --browser firefox --headless",
        "cy:e2e:localhost": "cypress run --spec \"cypress/integration/e2e/**/*.test.js\" --browser firefox --headless",
        "cy:e2e:mocked-backend": "cypress run --spec \"cypress/integration/mocked-e2e/**/*.test.js\" --env catalogHomePage=https://localhost:3000/ui/v1/apicagalog,username=user,password=user",
        "the-scripts-below-might-be-stale": "",
        "start": "cross-env HTTPS=true react-scripts start",
        "start:dev": "concurrently -c blue,green -n Backend,UI \"npm run start:mock\" \"cross-env REACT_APP_GATEWAY_URL=http://localhost:8000 npm run start\"",
        "start:mock": "cross-env REACT_APP_GATEWAY_URL=http://localhost:8000 nodemon mock/server.js",
        "analyze": "source-map-explorer build/static/js/main.*",
        "delete:reports": "rm ./test-results/e2e/* || true"
    },
    "proxy": "https://localhost:10010",
    "jest-html-reporter": {
        "pageTitle": "API Catalog UI Unit Tests",
        "outputPath": "./test-results/test-report-unit.html",
        "includeFailureMsg": true
    },
    "devDependencies": {
        "@babel/eslint-parser": "7.18.2",
        "@babel/preset-react": "7.18.6",
        "@testing-library/dom": "8.11.1",
        "@testing-library/jest-dom": "5.16.1",
        "@testing-library/react": "12.1.2",
        "@testing-library/user-event": "13.5.0",
        "ajv": "^7.2.4",
        "ansi-regex": "5.0.1",
        "body-parser": "1.19.2",
        "concurrently": "5.3.0",
        "cors": "2.8.5",
        "cross-env": "5.2.0",
        "cypress": "6.5.0",
        "enzyme": "3.7.0",
        "enzyme-adapter-react-16": "1.15.6",
        "eslint": "7.32.0",
        "eslint-config-airbnb": "19.0.4",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-flowtype": "5.7.2",
        "eslint-plugin-header": "3.1.1",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-jsx-a11y": "6.6.0",
        "eslint-plugin-prettier": "3.4.0",
        "eslint-plugin-react": "7.30.1",
        "express": "4.17.3",
        "jest": "26.6.0",
        "jest-environment-enzyme": "7.1.2",
        "jest-enzyme": "7.1.2",
        "jest-html-reporter": "3.4.0",
        "jest-junit": "6.4.0",
        "jest-mock": "26.6.2",
        "jest-watch-typeahead": "0.6.5",
        "json-schema": "0.4.0",
        "mini-css-extract-plugin": "2.4.7",
        "nodemon": "2.0.18",
        "prettier": "2.3.0",
        "react-scripts": "5.0.1",
        "react-testing-library": "5.3.0",
        "redux-mock-store": "1.5.4",
        "rimraf": "3.0.2",
        "source-map-explorer": "2.3.1",
        "start-server-and-test": "1.14.0",
        "tmpl": "1.0.5",
        "yaml": "1.10.2"
    },
    "jest": {
        "transformIgnorePatterns": [
            "/node_modules/?!(swagger-ui-react/swagger-ui-es-bundle-core.js)"
        ],
        "collectCoverageFrom": [
            "src/App.{jsx,js}",
            "src/**/*.{jsx,js}",
            "src/**/reducers/*.{jsx,js}",
            "!src/index.js",
            "!src/**/*Container.jsx",
            "!src/responsive-tests/**",
            "!cypress/*"
        ]
    },
    "engines": {
        "npm": "=9.3.1",
        "node": "=18.14.0"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
