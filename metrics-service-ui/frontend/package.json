{
    "name": "metrics-frontend",
    "version": "0.3.0",
    "private": true,
    "homepage": ".",
    "dependencies": {
        "@babel/core": "7.14.2",
        "@material-ui/core": "4.11.4",
        "@material-ui/icons": "4.11.2",
        "@wojtekmaj/enzyme-adapter-react-17": "0.6.1",
        "d3": "^7.0.0",
        "eslint": "7.11.0",
        "history": "4.10.1",
        "jquery": "^3.6.0",
        "lodash": "4.17.21",
        "loglevel": "1.7.1",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-loadable": "5.5.0",
        "react-redux": "7.2.4",
        "react-router-dom": "5.2.0",
        "react-scripts": "4.0.3",
        "redux": "4.1.0",
        "redux-catch": "1.3.1",
        "redux-logger": "3.0.6",
        "redux-observable": "1.2.0",
        "redux-persist": "6.0.0",
        "redux-persist-transform-filter": "0.0.20",
        "redux-thunk": "2.3.0",
        "rxjs": "6.6.7",
        "tinysort": "^3.2.7",
        "underscore": "^1.13.1",
        "uuid": "8.3.2"
    },
    "devDependencies": {
        "cypress": "7.4.0",
        "enzyme": "3.11.0",
        "eslint-config-airbnb": "16.1.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-flowtype": "5.7.2",
        "eslint-plugin-import": "2.23.2",
        "eslint-plugin-jsx-a11y": "6.1.2",
        "eslint-plugin-prettier": "3.4.0",
        "eslint-plugin-react": "7.23.2",
        "jest": "26.6.0",
        "jest-enzyme": "7.1.2",
        "jest-html-reporter": "3.4.0",
        "jest-mock": "26.6.2",
        "prettier": "2.3.0"
    },
    "scripts": {
        "build": "react-scripts build",
        "test": "react-scripts test --watchAll=false --env=jsdom --reporters=default --reporters=jest-html-reporter --coverage --silent",
        "start": "react-scripts start",
        "cy:open": "cypress open",
        "cy:e2e": "cypress run --spec \"cypress/integration/e2e/**/*.test.js\" --browser firefox --headless"
    },
    "jest": {
        "transformIgnorePatterns": [
            "<roodDir>/node_modules/(?!@swish)"
        ]
    },
    "jest-html-reporter": {
        "pageTitle": "Metrics Service UI Unit Tests",
        "outputPath": "./test-results/test-report-unit.html",
        "includeFailureMsg": true
    },
    "engines": {
        "node": "=10.23.3"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
